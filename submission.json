        "internalPort": 5432
      },
      "backend": {
        "name": "backend",
        "dockerfile": "backend/Dockerfile",
        "externalPort": 5000,
        "internalPort": 5000,
        "healthCheck": "/api/health"
      },
      "frontend": {
        "name": "frontend",
        "dockerfile": "frontend/Dockerfile",
        "externalPort": 3000,
        "internalPort": 3000
      }
    }
  },
